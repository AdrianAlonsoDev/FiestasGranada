<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="360"
                android:endColor="#01AC08"
                android:startColor="#00FF0A"
                android:type="sweep" />
            <size
                android:width="24dp"
                android:height="24dp" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorBlueMenu" />
            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>